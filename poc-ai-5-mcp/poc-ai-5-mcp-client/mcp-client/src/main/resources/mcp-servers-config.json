{
  "mcpServers": {
    "filesystem": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        ".../java/POC-IA/poc-ai-5-mcp/poc-ai-5-mcp-client/mcp-client/mcp"
      ]
    },
    "github": {
      "command": "docker",
      "args": [
